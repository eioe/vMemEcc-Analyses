
dat <- read_csv2('C:/Users/Felix/Seafile/Experiments/CentralKollegs18/centralkollegs18/Data/VRTask/Logfiles/ExpSubjects/S25/S25_PlayerLog_10-03-48-04-03-2019.txt', skip = 6)

